{
  "manifest_version": 2,

  "name": "SearchBuddy",
  "description": "This extension saves and annotates your Google research.",
  "version": "1.0",
    "icons":{"128": "search_bud.png"},
  "browser_action": {
	"default_title": "SearchBuddy",
    "default_icon": "icon.png",
    "default_popup": "popup.html",
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
